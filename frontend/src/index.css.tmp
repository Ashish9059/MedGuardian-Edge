@import url('https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&family=IBM+Plex+Mono:wght@400;500;600&display=swap');

:root {
  /* High-Performance Medical Palette (v3.0) */
  --h-primary: 204;
  --s-primary: 90%;
  --l-primary: 50%;

  --accent: hsl(var(--h-primary), var(--s-primary), var(--l-primary));
  --accent-glow: hsla(var(--h-primary), var(--s-primary), var(--l-primary), 0.25);
  --accent-hover: hsl(var(--h-primary), var(--s-primary), 65%);

  --bg-deep: #05070a;
  --bg-glass: rgba(13, 17, 23, 0.75);
  --bg-card: #0d1117;

  --border: rgba(255, 255, 255, 0.05);
  --border-bright: rgba(255, 255, 255, 0.12);

  --text: #f0f4f8;
  --text-muted: #8ba2b9;
  --text-dim: #4d667d;

  --clinical-cyan: #00f2ff;
  --clinical-red: #ff3b3b;
  --clinical-green: #00ff88;

  --radius: 16px;
  --radius-sm: 10px;
  --shadow-ux: 0 20px 50px -12px rgba(0, 0, 0, 0.8);

  --font-main: 'Inter', -apple-system, sans-serif;
  --font-mono: 'IBM Plex Mono', monospace;

  --glass-stroke: linear-gradient(135deg, rgba(255, 255, 255, 0.15), rgba(255, 255, 255, 0.02));
  --inner-glow: inset 0 0 0 1px rgba(255, 255, 255, 0.03);

  --anim-ease: cubic-bezier(0.2, 0.8, 0.2, 1);
}

:focus-visible {
  outline: 2px solid var(--accent);
  outline-offset: 4px;
}

*,
*::before,
*::after {
  box-sizing: border-box;
  margin: 0;
  padding: 0;
}

html {
  font-size: 16px;
  scroll-behavior: smooth;
}

body {
  font-family: var(--font-main);
  background: var(--bg-deep);
  color: var(--text);
  min-height: 100vh;
  line-height: 1.6;
  overflow-x: hidden;
  -webkit-font-smoothing: antialiased;
}

/* 🌀 Immersive High-Fidelity Mesh Background */
.mesh-bg {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: -1;
  background: var(--bg-deep);
  overflow: hidden;
}

.mesh-bg::before,
.mesh-bg::after {
  content: "";
  position: absolute;
  width: 140vmax;
  height: 140vmax;
  top: -20vmax;
  left: -20vmax;
  background-image:
    radial-gradient(circle at 20% 30%, hsla(var(--h-primary), 90%, 50%, 0.1) 0%, transparent 40%),
    radial-gradient(circle at 80% 70%, hsla(260, 80%, 50%, 0.1) 0%, transparent 40%),
    radial-gradient(circle at 50% 50%, hsla(var(--h-primary), 90%, 60%, 0.05) 0%, transparent 60%);
  filter: blur(80px);
  animation: mesh-float 30s infinite alternate ease-in-out;
}

.mesh-bg::after {
  left: auto;
  right: -20vmax;
  animation-duration: 45s;
  animation-direction: alternate-reverse;
  opacity: 0.6;
}

@keyframes mesh-float {
  0% {
    transform: rotate(0deg) scale(1);
  }

  50% {
    transform: rotate(5deg) scale(1.1);
  }

  100% {
    transform: rotate(-5deg) scale(1);
  }
}

/* 🧊 Glassmorphism 3.0 (Frosted Noise) */
.glass-3-0 {
  background: var(--bg-glass);
  backdrop-filter: blur(20px) saturate(180%);
  -webkit-backdrop-filter: blur(20px) saturate(180%);
  border: 1px solid var(--border);
  box-shadow: var(--shadow-ux), var(--inner-glow);
  position: relative;
  overflow: hidden;
}

.glass-3-0::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-image: url("data:image/svg+xml,%3Csvg viewBox='0 0 200 200' xmlns='http://www.w3.org/2000/svg'%3E%3Cfilter id='noiseFilter'%3E%3CfeTurbulence type='fractalNoise' baseFrequency='0.65' numOctaves='3' stitchTiles='stitch'/%3E%3C/filter%3E%3Crect width='100%25' height='100%25' filter='url(%23noiseFilter)'/%3E%3C/svg%3E");
  opacity: 0.015;
  pointer-events: none;
  z-index: 1;
}

/* 🧬 Clinical Typography */
.clinical-data {
  font-family: var(--font-mono);
  letter-spacing: -0.02em;
  font-variant-numeric: tabular-nums;
  color: var(--clinical-cyan);
}

/* ─── Layout ─────────────────────────────── */
.app {
  max-width: 1440px;
  margin: 0 auto;
  padding: 0 2rem 4rem;
}

/* ─── Header ─────────────────────────────── */
.app-header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 2rem 0;
  border-bottom: 1px solid var(--border);
  margin-bottom: 2.5rem;
}

.dock-item {
  width: 44px;
  height: 44px;
  border-radius: 12px;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 1.2rem;
  color: var(--text-dim);
  cursor: pointer;
  transition: all 0.2s ease;
  background: rgba(255, 255, 255, 0.02);
  border: 1px solid transparent;
}

.dock-item:hover {
  background: rgba(255, 255, 255, 0.05);
  color: var(--text);
  border-color: var(--border-bright);
}

.dock-item.active {
  background: var(--accent-glow);
  color: var(--accent);
  border-color: var(--accent);
}

/* ─── Floating Protocol Island ───────────── */
.protocol-island {
  position: fixed;
  bottom: 2rem;
  left: 50%;
  transform: translateX(-50%);
  background: rgba(13, 17, 23, 0.8);
  backdrop-filter: blur(24px) saturate(180%);
  border: 1px solid var(--border-bright);
  padding: 0.75rem 1.5rem;
  border-radius: 999px;
  display: flex;
  align-items: center;
  gap: 1.5rem;
  box-shadow: 0 20px 50px rgba(0, 0, 0, 0.6), 0 0 20px rgba(var(--h-primary), 90%, 50%, 0.1);
  z-index: 1000;
  animation: island-float 1s var(--anim-ease) backwards;
}

@keyframes island-float {
  from {
    transform: translate(-50%, 40px);
    opacity: 0;
  }

  to {
    transform: translate(-50%, 0);
    opacity: 1;
  }
}

.island-btn {
  background: transparent;
  border: none;
  color: var(--text-muted);
  font-weight: 600;
  font-size: 0.85rem;
  display: flex;
  align-items: center;
  gap: 0.5rem;
  cursor: pointer;
  padding: 0.5rem 1rem;
  border-radius: 999px;
  transition: all 0.2s ease;
}

.island-btn:hover {
  background: rgba(255, 255, 255, 0.05);
  color: var(--text);
}

.island-btn-primary {
  background: linear-gradient(135deg, var(--accent), #7c3aed);
  color: #fff;
  box-shadow: 0 4px 15px var(--accent-glow);
}

.island-separator {
  width: 1px;
  height: 24px;
  background: var(--border);
}

/* 🌀 Organic Glow Anchors */
.glow-anchor {
  position: absolute;
  width: 300px;
  height: 300px;
  background: radial-gradient(circle, hsla(var(--h-primary), 90%, 50%, 0.15) 0%, transparent 70%);
  filter: blur(40px);
  pointer-events: none;
  z-index: 0;
  mix-blend-mode: plus-lighter;
}

.header-brand {
  display: flex;
  align-items: center;
  gap: 1rem;
}

.header-logo {
  font-size: 2.2rem;
  filter: drop-shadow(0 0 8px var(--accent-glow));
}

.app-header h1 {
  font-size: 1.75rem;
  font-weight: 700;
  letter-spacing: -0.02em;
  background: linear-gradient(to right, #ffffff, var(--accent));
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

.header-subtitle {
  font-size: 0.85rem;
  color: var(--text-muted);
  font-weight: 500;
  opacity: 0.8;
}

/* ─── Status Badge ───────────────────────── */
.status-badge {
  display: flex;
  align-items: center;
  gap: 0.6rem;
  padding: 0.5rem 1rem;
  border-radius: 999px;
  font-size: 0.75rem;
  font-weight: 600;
  text-transform: uppercase;
  letter-spacing: 0.05em;
  background: rgba(255, 255, 255, 0.03);
  border: 1px solid var(--border);
  backdrop-filter: blur(4px);
}

.status-dot {
  width: 7px;
  height: 7px;
  border-radius: 50%;
  background: var(--text-dim);
}

.status-badge.connected .status-dot {
  background: var(--success);
  box-shadow: 0 0 10px var(--success);
}

/* ─── Cards (Glassmorphism 2.0) ──────────── */
.card {
  background: var(--bg-glass);
  backdrop-filter: blur(14px);
  -webkit-backdrop-filter: blur(14px);
  border: 1px solid var(--border);
  box-shadow: var(--shadow-lg), var(--inner-glow);
  border-radius: var(--radius);
  padding: 1.5rem;
  position: relative;
  overflow: hidden;
  transition: transform 0.4s var(--anim-ease), border-color 0.4s var(--anim-ease), box-shadow 0.4s var(--anim-ease);
}

.card::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  border-radius: inherit;
  padding: 1px;
  background: var(--glass-stroke);
  -webkit-mask: linear-gradient(#fff 0 0) content-box, linear-gradient(#fff 0 0);
  mask: linear-gradient(#fff 0 0) content-box, linear-gradient(#fff 0 0);
  -webkit-mask-composite: xor;
  mask-composite: exclude;
  pointer-events: none;
}

.card:hover {
  border-color: var(--border-bright);
}

.card-header-flex {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 1.5rem;
}

.dock-status {
  margin-top: auto;
  padding-bottom: 2rem;
  display: flex;
  justify-content: center;
  align-items: center;
}

.dock-status .status-dot {
  width: 10px;
  height: 10px;
  border-radius: 50%;
  background: var(--text-dim);
  box-shadow: 0 0 0 4px rgba(255, 255, 255, 0.02);
}

.dock-status.connected .status-dot {
  background: var(--clinical-green);
  box-shadow: 0 0 12px var(--clinical-green), 0 0 0 4px rgba(0, 255, 136, 0.1);
}

.island-error {
  color: var(--clinical-red);
  font-size: 0.75rem;
  font-weight: 700;
  text-transform: uppercase;
  letter-spacing: 0.05em;
}

/* ─── Main Grid (Wide Clinical Layout) ────── */
.app-main {
  display: grid;
  grid-template-columns: 1fr 1fr;
  gap: 2rem;
  margin-bottom: 2rem;
}

.left-column,
.right-column {
  display: flex;
  flex-direction: column;
  gap: 2rem;
}

/* ─── Form Controls ──────────────────────── */
.form-group {
  margin-bottom: 1.25rem;
}

.form-group label {
  display: block;
  font-size: 0.8rem;
  font-weight: 600;
  color: var(--text-muted);
  margin-bottom: 0.5rem;
}

.hint {
  font-weight: 400;
  color: var(--text-dim);
  font-size: 0.75rem;
}

input,
select,
textarea {
  background: rgba(0, 0, 0, 0.2);
  border: 1px solid var(--border);
  border-radius: var(--radius-sm);
  color: var(--text);
  padding: 0.75rem 1rem;
  font-size: 0.9rem;
  transition: all 0.2s cubic-bezier(0.4, 0, 0.2, 1);
  outline: none;
}

input:focus,
select:focus,
textarea:focus {
  border-color: var(--accent);
  background: rgba(0, 0, 0, 0.3);
  box-shadow: 0 0 0 3px var(--accent-glow);
}

/* ─── Action Bar & Buttons ────────────────── */
/* ─── MedGuardian Branding ───────────────── */
.app-header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 2rem 0;
  border-bottom: 1px solid var(--border);
  margin-bottom: 2.5rem;
}

.header-brand {
  display: flex;
  align-items: center;
  gap: 1.5rem;
}

.tag-v2 {
  font-size: 0.65rem;
  padding: 0.2rem 0.5rem;
  background: var(--accent-glow);
  color: var(--accent);
  border-radius: 4px;
  vertical-align: middle;
  margin-left: 0.5rem;
  border: 1px solid hsla(var(--h-primary), 90%, 50%, 0.2);
}

.btn-analyze {
  position: relative;
  background: linear-gradient(135deg, var(--accent), #7c3aed);
  color: #fff;
  border: none;
  border-radius: var(--radius-sm);
  padding: 0.8rem 2.5rem;
  font-size: 1rem;
  font-weight: 600;
  cursor: pointer;
  box-shadow: 0 4px 15px rgba(79, 142, 247, 0.4);
  transition: all 0.3s ease;
  overflow: hidden;
}

.btn-analyze:hover:not(:disabled) {
  transform: translateY(-2px);
  box-shadow: 0 8px 25px rgba(79, 142, 247, 0.5);
  filter: brightness(1.1);
}

.btn-analyze:active:not(:disabled) {
  transform: translateY(0);
}

.btn-analyze:disabled {
  opacity: 0.4;
  cursor: not-allowed;
  filter: grayscale(0.5);
}

.btn-reset {
  background: rgba(255, 255, 255, 0.03);
  border: 1px solid var(--border);
  color: var(--text-muted);
  padding: 0.8rem 1.5rem;
  border-radius: var(--radius-sm);
  font-size: 0.9rem;
  font-weight: 500;
  transition: all 0.2s ease;
}

.btn-reset:hover {
  background: rgba(255, 255, 255, 0.08);
  color: #fff;
}

/* ─── Results Panel ──────────────────────── */
.results-container {
  margin-top: 2rem;
}

.results-panel {
  background: var(--bg-glass);
  backdrop-filter: blur(16px);
  border-radius: var(--radius);
  border: 1px solid var(--border);
  padding: 2rem;
  box-shadow: var(--shadow-lg);
}

/* ─── Tabs ───────────────────────────────── */
.tabs {
  display: flex;
  gap: 0.5rem;
  margin-bottom: 2rem;
  padding-bottom: 2px;
  overflow-x: auto;
}

/* ─── Dashboard Sophistication ───────────── */
.clinical-table {
  width: 100%;
  border-collapse: separate;
  border-spacing: 0 0.5rem;
  margin: 1rem 0;
}

.clinical-table tr {
  background: rgba(255, 255, 255, 0.02);
  transition: all 0.2s ease;
}

.clinical-table tr:hover {
  background: rgba(var(--h-primary), 90%, 50%, 0.05);
  transform: scale(1.005);
}

.clinical-table th {
  text-align: left;
  padding: 0.75rem 1rem;
  font-size: 0.7rem;
  font-weight: 700;
  color: var(--text-dim);
  text-transform: uppercase;
  letter-spacing: 0.1em;
}

.clinical-table td {
  padding: 1rem;
  border-top: 1px solid var(--border);
  border-bottom: 1px solid var(--border);
}

.clinical-table td:first-child {
  border-left: 1px solid var(--border);
  border-top-left-radius: 8px;
  border-bottom-left-radius: 8px;
}

.clinical-table td:last-child {
  border-right: 1px solid var(--border);
  border-top-right-radius: 8px;
  border-bottom-right-radius: 8px;
}

.spark-tag {
  display: inline-flex;
  align-items: center;
  padding: 0.2rem 0.5rem;
  background: rgba(var(--h-primary), 90%, 50%, 0.1);
  color: var(--accent);
  border-radius: 4px;
  font-size: 0.65rem;
  font-weight: 700;
  margin-left: 0.5rem;
}

.tab-btn.tab-active {
  background: linear-gradient(135deg, var(--accent), #7c3aed) !important;
  color: #fff !important;
  border: none !important;
  box-shadow: 0 8px 20px var(--accent-glow);
}

.empty-state {
  padding: 4rem 2rem;
  text-align: center;
  background: rgba(255, 255, 255, 0.01);
  border: 1px dashed var(--border);
  border-radius: var(--radius);
  margin-top: 2rem;
}

.empty-icon {
  font-size: 3rem;
  display: block;
  margin-bottom: 1rem;
  filter: grayscale(1) opacity(0.3);
}

.empty-content p {
  color: var(--text-dim);
  font-weight: 500;
  letter-spacing: 0.02em;
}

/* ─── Badges ─────────────────────────────── */
.badge {
  padding: 0.3rem 0.75rem;
  border-radius: 6px;
  font-size: 0.7rem;
  font-weight: 700;
  letter-spacing: 0.05em;
  text-transform: uppercase;
}

.badge-vision {
  background: hsla(217, 91%, 60%, 0.15);
  color: var(--accent);
  border: 1px solid hsla(217, 91%, 60%, 0.3);
}

.badge-text-mode {
  background: rgba(255, 255, 255, 0.05);
  color: var(--text-muted);
  border: 1px solid var(--border);
}

/* ─── Multimodal Upload ──────────────────── */
.upload-zone {
  border: 2px dashed var(--border);
  background: rgba(255, 255, 255, 0.01);
  border-radius: var(--radius);
  padding: 2rem;
  transition: all 0.3s ease;
}

.upload-zone:hover {
  border-color: var(--accent);
  background: hsla(217, 91%, 60%, 0.04);
}

.upload-icon {
  font-size: 2rem;
  margin-bottom: 0.5rem;
  display: block;
  opacity: 0.7;
}

/* ─── Debug Panel ────────────────────────── */
.dev-debug-panel {
  background: rgba(0, 0, 0, 0.3);
  border: 1px dashed rgba(251, 191, 36, 0.3);
  padding: 1.25rem;
  border-radius: var(--radius-sm);
  margin-top: 2rem;
}

.dev-debug-panel h5 {
  color: #fbbf24;
  font-size: 0.8rem;
  margin-bottom: 0.75rem;
  text-transform: uppercase;
  letter-spacing: 0.05em;
}

/* ─── Responsive ─────────────────────────── */
@media (max-width: 1024px) {
  .app-main {
    grid-template-columns: 1fr;
  }
}

@media (max-width: 640px) {
  .app {
    padding: 0 1rem 3rem;
  }

  .app-header {
    flex-direction: column;
    align-items: flex-start;
    gap: 1rem;
  }
}

/* ─── Utilities ──────────────────────────── */
.divider-text {
  position: relative;
  text-align: center;
  margin: 2rem 0;
  font-size: 0.7rem;
  font-weight: 700;
  color: var(--text-dim);
  text-transform: uppercase;
  letter-spacing: 0.1em;
}

.divider-text::before,
.divider-text::after {
  content: "";
  position: absolute;
  top: 50%;
  width: 35%;
  height: 1px;
  background: var(--border);
}

.divider-text::before {
  left: 0;
}

.divider-text::after {
  right: 0;
}

.spinner {
  width: 18px;
  height: 18px;
  border: 2px solid rgba(255, 255, 255, 0.2);
  border-top: 2px solid #fff;
  border-radius: 50%;
  animation: spin 0.8s linear infinite;
}

@keyframes spin {
  from {
    transform: rotate(0deg);
  }

  to {
    transform: rotate(360deg);
  }
}

/* ─── Skeleton & Shimmer ─────────────────── */
.skeleton {
  background: linear-gradient(90deg,
      rgba(255, 255, 255, 0.03) 25%,
      rgba(255, 255, 255, 0.08) 50%,
      rgba(255, 255, 255, 0.03) 75%);
  background-size: 200% 100%;
  animation: shimmer 1.5s infinite var(--anim-ease);
  border-radius: 4px;
}

@keyframes shimmer {
  0% {
    background-position: 200% 0;
  }

  100% {
    background-position: -200% 0;
  }
}

.skeleton-text {
  height: 1rem;
  width: 100%;
  margin-bottom: 0.75rem;
}

.skeleton-title {
  height: 1.5rem;
  width: 60%;
  margin-bottom: 1.5rem;
}

.skeleton-rect {
  height: 100px;
  width: 100%;
}

/* ─── Entry Animations ───────────────────── */
.entry-fade {
  opacity: 0;
  transform: translateY(10px);
  animation: entryFade 0.6s var(--anim-ease) forwards;
}

@keyframes entryFade {
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

.stagger-1 {
  animation-delay: 0.1s;
}

.stagger-2 {
  animation-delay: 0.2s;
}

.stagger-3 {
  animation-delay: 0.3s;
}

/* ─── Progress Bar ───────────────────────── */
.analysis-progress-container {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  height: 3px;
  background: rgba(255, 255, 255, 0.05);
  z-index: 1000;
  overflow: hidden;
}

.analysis-progress-bar {
  height: 100%;
  background: linear-gradient(to right, var(--accent), #7c3aed);
  transition: width 0.3s ease;
  box-shadow: 0 0 10px var(--accent-glow);
}

.analysis-progress-bar.loading {
  animation: progress-indeterminate 2s infinite linear;
  width: 30%;
}

@keyframes progress-indeterminate {
  0% {
    transform: translateX(-100%);
  }

  100% {
    transform: translateX(400%);
  }
}
